{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
		{ "source": "/listall", "destination": "/api/listall.rs" },
		{ "source": "/create", "destination": "/api/create.rs" },
		{ "source": "/update", "destination": "/api/update.rs" },
		{ "source": "/login", "destination": "/api/login.rs" },
		{ "source": "/delete", "destination": "/api/delete.rs" },
		{ "source": "/user/(.*)", "destination": "/api/user/[id].rs" }
    ],
    "functions": {
      "api/**/*.rs": {
        "runtime": "vercel-rust@4.0.6"
      }
    },
    "headers": [
      {
        "source": "(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "POST, GET"
          },
          {
            "key": "Author",
            "value": "visnkmr@github"
          },
          {
            "key": "Github",
            "value": "https://github.com/visnkmr/listallfrompscale"
          }
        ]
      },
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "POST, GET"
          },
          {
            "key": "Author",
            "value": "visnkmr@github"
          },
          {
            "key": "Github",
            "value": "https://github.com/visnkmr/listallfrompscale"
          }
        ]
      }
    ]
}
